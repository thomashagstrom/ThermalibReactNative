{
  "name": "ThermalibReactNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:gen": "cd android && ./gradlew generateCodegenArtifactsFromSchema",
    "adb:connect": "adb reverse tcp:8081 tcp:5555",
    "ios": "react-native run-ios",
    "ios:gen": "cd ios && bundle instalL && bundle expec pod install",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "codegenConfig": {
    "name": "NativeThermalib",
    "type": "modules",
    "jsSrcsDir": "specs",
    "android": {
      "javaPackageName": "com.thermalibreactnative"
    }
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.76.0"
  },
  "repository": "https://github.com/thomashagstrom/ThermalibReactNative",
  "bugs": {
    "url": "https://github.com/thomashagstrom/ThermalibReactNative/issues"
  },
  "author": "Thomas HagstroÌˆm <thomashagstrom@outlook.com> (https://github.com/thomashagstrom)",
  "license": "MIT",
  "homepage": "https://github.com/thomashagstrom/ThermalibReactNative#readme",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.0-alpha.2",
    "@react-native-community/cli-platform-android": "15.0.0-alpha.2",
    "@react-native-community/cli-platform-ios": "15.0.0-alpha.2",
    "@react-native/babel-preset": "0.76.0",
    "@react-native/eslint-config": "0.76.0",
    "@react-native/metro-config": "0.76.0",
    "@react-native/typescript-config": "0.76.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
